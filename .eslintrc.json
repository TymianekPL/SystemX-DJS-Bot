{
     "env": {
          "commonjs": true,
          "es2021": true,
          "node": true
     },
     "extends": ["eslint:recommended", "prettier"],
     "overrides": [],
     "parserOptions": {
          "ecmaVersion": "latest"
     },
     "rules": {
          "indent": ["error", 5],
          "linebreak-style": ["error", "windows"],
          "quotes": ["error", "double"],
          "semi": ["error", "always"]
     }
}
